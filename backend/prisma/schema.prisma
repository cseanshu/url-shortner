generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model URL {
  id        Int      @id @default(autoincrement())
  longUrl   String
  shortCode String   @unique
  clicks    Int      @default(0)
  createdAt DateTime @default(now())
}
